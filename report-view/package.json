{
  "name": "report-view",
  "displayName": "Report View",
  "description": "Report(Duplicate„ÄÅUnused code and file) view vscode extension.",
  "version": "0.0.1",
  "publisher": "Nomeleel",
  "repository": "https://github.com/Nomeleel/VS-Code-Extension-Camp/report-view",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pubspec.yaml"
  ],
  "main": "./out/main",
  "contributes": {
    "commands": [
      {
        "command": "report-view.deleteFile",
        "title": "Delete File",
        "icon": "./resource/icons/delete.png"
      }
    ],
    "views": {
      "flutter": [
        {
          "id": "unused.code.view",
          "name": "Unused Code View",
          "contextualTitle": "Unused Code View"
        },
        {
          "id": "unused.file.view",
          "name": "Unused File View",
          "contextualTitle": "Unused File View"
        },
        {
          "id": "duplicate.view",
          "name": "Duplicate View",
          "contextualTitle": "Duplicate View"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "report-view.deleteFile",
          "when": "view == unused.file.view",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --pure:console.log --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "watch": "npm run esbuild"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@types/vscode": "^1.76.0",
    "esbuild": "^0.17.10",
    "typescript": "^4.9.5"
  }
}
