{
	"name": "magic",
	"displayName": "Magic",
	"description": "Magic vs code extension.",
	"version": "0.0.1",
	"publisher": "Nomeleel",
	"repository": {
		"type": "git",
		"url": "https://github.com/Nomeleel/VS-Code-Extension-Collection.git"
	},
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:json",
		"workspaceContains:*.json",
		"workspaceContains:**/*.json",
		"onCommand:magic.showTime"
	],
	"main": "./out/extension/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "json",
				"path": "./snippets/magic.json"
			},
			{
				"language": "dart",
				"path": "./snippets/script.json"
			}
		],
		"commands": [
			{
				"command": "magic.showTime",
				"title": "Show Time ^_^"
			},
			{
				"command": "magic.addScript",
				"title": "Add Script",
				"category": "Dart"
			}
		],
		"grammars": [
			{
				"language": "json",
				"scopeName": "source.json",
				"path": "./syntaxes/JSON.tmLanguage.json"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "magic.addScript",
					"when": "explorerResourceIsFolder && resourceFilename == test",
					"group": "0_add_file"
				}
			]
		}
	},
	"scripts": {
		"start": "npm run compile",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}